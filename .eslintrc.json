{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "import/prefer-default-export": 0,
        "indent": ["error", 2],
        "no-param-reassign" : 0
    }
}
